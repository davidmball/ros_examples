<?xml version="1.0"?>
<example>
  <title>Marker publishing</title>
  <description>This example demonstrates how to display a marker.</description>
  <ros_version>kinetic</ros_version>
  <tag>intro</tag>
  <tag>publisher</tag>
  <tag>marker</tag>
  <time_limit>20</time_limit>
  <language>
    <name>cpp</name>
    <run_cmd>rosrun intro_marker_publisher marker_publisher</run_cmd>
    <start_file>/src/marker_publisher.cpp</start_file>
  </language>
  <feedback>
    <tab>viz</tab>
    <topic>/markers</topic>
    <frame>/the_frame</frame>
  </feedback>
  <feedback>
    <tab>terminal</tab>
  </feedback>
</example>
