<?xml version="1.0"?>
<example>
  <title>Pose publishing</title>
  <run_cmd>rosrun pose_publisher_loop pose_loop</run_cmd>
  <description>This example demonstrates how to publish a pose message to a topic.</description>
  <ros_version>kinetic</ros_version>
  <start_file>/src/pose_loop.cpp</start_file>
  <tag>intro</tag>
  <tag>publisher</tag>
  <tag>topic</tag>
  <tag>pose</tag>
  <feedback_top>
    <tab>topics</tab>
    <topic>/robot_pose</topic>
    <msg_type>geometry_msgs/PoseStamped</msg_type>
  </feedback_top>
  <feedback_bottom>
    <tab>terminal</tab>
  </feedback_bottom>
</example>
