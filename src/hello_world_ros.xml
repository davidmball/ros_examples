<?xml version="1.0"?>
<example>
  <title>Hello World</title>
  <description>This example prints hello world in the ROS 'printf' and stream way.</description>
  <ros_version>kinetic</ros_version>
  <tag>intro</tag>
  <tag>debug</tag>
  <time_limit>15</time_limit>
  <language>
    <name>cpp</name>
    <run_cmd>rosrun hello_world_ros hello_world_ros</run_cmd>
    <start_file>/src/hello_world.cpp</start_file>
  </language>
  <language>
    <name>py</name>
    <run_cmd>rosrun hello_world_ros hello_world.py</run_cmd>
    <start_file>/src/hello_world.py</start_file>
  </language>
  <feedback>
    <tab>terminal</tab>
  </feedback>
  <feedback>
    <tab>nodes</tab>
  </feedback>
</example>
